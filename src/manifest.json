{
  "manifest_version": 2,
  "name": "Auto Form Filler",
  "version": "1.0",
  "description": "Description of your extension",
  "permissions": ["storage", "activeTab"],
  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
